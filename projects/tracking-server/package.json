    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@forfuture/apidoc-openapi": "^0.0.1",
    "apidoc": "^0.50.4",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.1.8",
    "coveralls": "^3.1.1",
